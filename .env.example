# Telegram Bot配置
BOT_TOKEN=your_bot_token_here
SUPERADMIN_ID=your_telegram_user_id
ADMINS_ID=admin1_id,admin2_id

# 机器人设置
BOT_NICKNAME=桜色服务助手
GROUP=your_group_username

# 数据库配置
DATABASE_URL=sqlite:///./db/bot.db
# 如果使用PostgreSQL，取消注释下面的行并注释上面的SQLite配置
# DATABASE_URL=postgresql+asyncpg://botuser:password@localhost:5432/lustfulservicebot

# 日志配置
LOG_LEVEL=INFO
DEBUG=False

# 可选：Redis配置（如果需要缓存）
# REDIS_URL=redis://localhost:6379/0

# 可选：文件上传配置
# MAX_FILE_SIZE=50MB
# UPLOAD_PATH=./uploads

# 可选：API配置
# API_HOST=0.0.0.0
# API_PORT=8000